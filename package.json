{
  "name": "wot-design",
  "version": "1.2.0",
  "description": "Mobile UI components built on vue.js",
  "scripts": {
    "dev": "npm run build:files & node build/build-install.js && cross-env NODE_ENV=development webpack-dev-server --progress --config build/webpack.docs.conf.js",
    "build:docs": "rimraf examples/dist && node build/build-install.js && webpack --progress --config build/webpack.docs.conf.js",
    "build:lib": "webpack --progress --config build/webpack.lib.conf.js",
    "build:components": "webpack --progress --config build/webpack.components.conf.js",
    "build:utils": "cross-env BABEL_ENV=utils babel src --out-dir lib --ignore src/index.js",
    "build:theme": "node build/style/mixCssFile.js & gulp build --gulpfile build/style/build-theme.js && node build/style/build-style-entry.js",
    "build:locale": "node build/build-locale.js",
    "build:files": "node build/style/iconInit.js & node build/build-entry.js",
    "dist": "rimraf lib && npm run build:files && npm run build:lib && npm run build:components && npm run build:theme && npm run build:utils && npm run build:locale",
    "unit": "jest --coverage"
  },
  "main": "./lib/index.js",
  "keywords": [
    "vue",
    "wot",
    "component"
  ],
  "files": [
    "lib",
    "packages",
    "src"
  ],
  "unpkg": "lib/index.js",
  "style": "lib/theme-chalk/index.css",
  "repository": {
    "type": "git",
    "url": "git@github.com:jd-ftf/wot-design.git"
  },
  "homepage": "http://jdftf.top/wot-design/",
  "peerDependencies": {
    "vue": "^2.5.2"
  },
